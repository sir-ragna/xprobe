CC=@CC@
CXX=@CXX@
INSTALL=@INSTALL@
INSTALL_PROGRAM=@INSTALL_PROGRAM@
INSTALL_DATA=@INSTALL_DATA@
DESTDIR=
prefix=@prefix@
exec_prefix=@exec_prefix@
bindir=@bindir@
mandir=@mandir@
PACKAGE=@PACKAGE@
VERSION=@VERSION@
CPPFLAGS=@CPPFLAGS@
CXXFLAGS=$(CPPFLAGS) @CXXFLAGS@ -I ../ -I ../../libs-external/USI++/
CFLAGS=$(CPPFLAGS) @CFLAGS@ -I ../ -I ../../libs-external/USI++/
LDFLAGS=@LDFLAGS@
LIBS=@LIBS@
INSTALL=@INSTALL@
RANLIB=@RANLIB@
OBJS=AsyncOutputReader.o PopenCommandExecutor.o PxprobeOutputProcessor.o pxprobe.o ../cmd_opts.o ../config_set.o ../interface.o ../interface_con.o ../log.o ../os_matrix.o ../scan_engine.o ../target.o ../targets_list.o ../xprobe_module_hdlr.o ../xprobe_module_param.o  ../xpmodules/alive_probe/alive_probe.a ../xpmodules/os_probe/os_probe.a ../xplib/libxplib.a ../../libs-external/USI++/src/libusi++.a


all: pxprobe

pxprobe: $(OBJS)
	$(CXX) $(CFLAGS) $(OBJS) $(MODOBJS) -o $@ $(LDFLAGS) $(LIBS)
	strip $@


.c.o: $(INCLUDES)
	$(CC) -c $(CFLAGS) $<

.cc.o: $(INCLUDES)
	$(CXX) -c $(CFLAGS) $<

clean:
	rm -f pxprobe core *.o

distclean: clean
	rm -f Makefile
